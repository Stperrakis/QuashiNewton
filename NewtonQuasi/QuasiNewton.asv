function min = QuasiNewton(f,start,e)

xk= num2cell(start);

itt = 1;
param = symvar(f);
grad = gradient(f,param);

dgradv = double(norm(grad(xk{:}),2));


while dgradv>=e
    
    H = hessian(f);
    H = H(xk{:});
    pk = -((inv(H)))*((grad(xk{:})));
    
    xk = xk + ak*pk;
    f(xk{:});
    
end

min = xk;

end